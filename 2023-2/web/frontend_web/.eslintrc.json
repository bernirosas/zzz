{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off",
        "max-len": "off",
        "no-alert": "off",
        "no-sequences": "off",
        "no-unused-expressions": "off",
        "no-use-before-define": "off",
        "eqeqeq": "off",
        "no-undef": "off",
        "consistent-return": "off",
        "no-underscore-dangle": "off",
        "no-nested-ternary": "off",
        "no-param-reassign": "off",
        "no-return-assign": "off",
        "func-names": "off",
        "no-plusplus": "off",
        "prefer-spread": "off",
        "new-cap": "off",
        "no-mixed-operators": "off",
        "no-void": "off",
        "no-continue": "off",
        "no-bitwise": "off",
        "valid-typeof": "off",
        "no-cond-assign": "off",
        "no-empty": "off",
        "prefer-destructuring": "off",
        "prefer-rest-params":"off",
        "no-multi-assign": "off",
        "class-methods-use-this": "off",
        "no-restricted-globals": "off",
        "prefer-const": "off",
        "no-new": "off",
        "no-vars": "off",
        "no-var": "off",
        "vars-on-top": "off",
        "guard-for-in": "off", 
        "no-constant-condition": "off",
        "no-restricted-syntax": "off",
        "no-func-assign": "off",
        "no-useless-concat": "off",
        "no-useless-return": "off",
        "no-labels": "off",
        "default-case": "off",
        "block-scoped-var": "off",
        "no-label-var": "off",
        "no-fallthrough": "off",
        "no-prototype-builtins": "off",
        "no-useless-escape": "off",
        "no-self-compare": "off",
        "max-classes-per-file": "off",
        "no-misleading-character-class": "off",
        "no-control-regex": "off",
        "getter-return": "off",
        "no-unexpected-token": "off"
    }
}
