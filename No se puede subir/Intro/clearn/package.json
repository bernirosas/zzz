{
  "name": "clearn",
  "version": "0.1.0",
  "description": "The frontend for handling coding exercises submission",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:test": "NEXTAUTH_URL=http://localhost:3001/ NEXT_PUBLIC_GRAPHQL_ENDPOINT=http://localhost:4002/ next dev -p 3001",
    "dev:test:win32": "set NEXTAUTH_URL=http://localhost:3001/ & set NEXT_PUBLIC_GRAPHQL_ENDPOINT=http://localhost:4002/ & next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "npm run _prettier -- --write",
    "check": "npm run _prettier -- --list-different; npm run lint",
    "_prettier": "prettier **/*.{tsx,ts,js,html,css} --ignore-path .gitignore",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate": "graphql-codegen --config codegen.yml -r dotenv/config",
    "cypress": "start-server-and-test dev:test http://localhost:3001 \"cypress open --e2e\"",
    "cypress:headless": "start-server-and-test dev:test http://localhost:3001 \"cypress run --e2e\"",
    "cypress:win32": "start-server-and-test dev:test:win32 http://localhost:3001 \"cypress open --e2e\"",
    "cypress:headless:win32": "start-server-and-test dev:test:win32 http://localhost:3001 \"cypress run --e2e\"",
    "update-service-worker": "bash -c \"cat node_modules/react-py/dist/workers/service-worker.js > public/react-py-sw.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IIC1103/clearn.git"
  },
  "keywords": [
    "education",
    "classroom",
    "frontend"
  ],
  "author": "Nicolas Quiroz",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/IIC1103/clearn/issues"
  },
  "homepage": "https://github.com/IIC1103/clearn#readme",
  "dependencies": {
    "@headlessui/react": "1.7.10",
