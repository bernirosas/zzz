{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "requireConfigFile": false,
        "babelOptions": {
            "presets": ["@babel/preset-react"]}
    },
    "rules": {"import/no-extraneous-dependencies": [
        "error",
        {"devDependencies": true}
      ],
      "no-console": "off",
      "camelcase": "off",
      "import/no-cycle": "off"
    }
}
